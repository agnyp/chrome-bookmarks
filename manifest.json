{
  "name": "Chrome bookmarks",
  "version": "0.1",
  "description": "Bookmarks as they are meant.",
  "permissions": [
    "declarativeContent",
    "bookmarks",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
